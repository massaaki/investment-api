{
  "name": "investment-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/massaaki/investment-api.git",
  "author": "massaaki <eu@massaaki.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn  src/main/server.ts",
    "test": "jest --passWithNoTests",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:unit": "yarn test -c jest.unit-config.js -- --watch",
    "test:integration": "yarn test -c jest.unit-config.js -- --watch",
    "test:cache": "jest --clearCache",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/better-sqlite3": "^7.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "git-commit-msg-linter": "^4.0.7",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "pg": "^8.7.1",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}